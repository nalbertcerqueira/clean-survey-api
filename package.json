{
    "name": "survey-api",
    "version": "2.0.0",
    "license": "MIT",
    "scripts": {
        "start": "node ./dist/main/server/server.js",
        "dev": "ts-node-dev --respawn --rs -r tsconfig-paths/register ./src/main/server/server.ts",
        "dev:debug": "ts-node-dev --respawn --rs --inspect=9229 -r tsconfig-paths/register ./src/main/server/server.ts",
        "build": "rm -rf ./dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
        "ts-lint": "tsc --noEmit && tsc-alias",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.1",
        "@commitlint/config-conventional": "^17.7.0",
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/node": "^20.5.9",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^9.0.3",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "@typescript-eslint/parser": "^6.6.0",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.3",
        "ts-node-dev": "^2.0.0",
        "tsc-alias": "^1.8.7",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@types/cors": "^2.8.14",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^6.0.0",
        "swagger-ui-express": "^5.0.0",
        "uuid": "^9.0.0",
        "yup": "^1.2.0"
    }
}
